            </field>  
            <field name = "bumpRate">
                <bind-xml name = "bumpRate" node = "attribute"/>
            </field>  
            <field name = "decayRate">
                <bind-xml name = "decayRate" node = "attribute"/>
            </field>  
            <field name = "inputThreshold">
                <bind-xml name = "inputThreshold" node = "attribute"/>
            </field>  
            <field name = "plasticityType">
