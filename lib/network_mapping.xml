            </field>
        </class>       

        <class name="org.simnet.interfaces.Synapse" identity = "id">
            <field name = "id">
                <bind-xml name = "id" node = "attribute"/>
            </field>  
            <field name = "source"> 
                <bind-xml name="src" node="attribute" reference="true"/>
