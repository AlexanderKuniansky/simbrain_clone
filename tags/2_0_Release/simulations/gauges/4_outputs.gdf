<?xml version="1.0" encoding="UTF-8"?>
<gauge-panel auto-zoom="true" color-mode="false" show-error="false"
    show-status="true" num-iterations-between-update="10"
    pointSize="0.4" backgroundColor="-1" hotColor="-65536" defaultColor="-16776961">
    <gauge>
        <projector xsi:type="project-pCA" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <projectorSettings tolerance="0.05" hiD1="0" hiD2="1"
                perturbation-amount="0.1" auto-find="true" epsilon="3.0" add-method="Refresh"/>
            <downstairs>
                <point> 0.00, 0.00</point>
                <point> 0.70, -0.70</point>
                <point> -0.70, -0.70</point>
            </downstairs>
            <upstairs>
                <point> 0.00, 0.00</point>
                <point> 0.00, 1.00</point>
                <point> 1.00, 0.00</point>
            </upstairs>
        </projector>
        <gaugedVariables network="FF-2-2.xml">
            <variables>n_4,n_3</variables>
        </gaugedVariables>
    </gauge>
</gauge-panel>
